ick ? 'nav-menu active' : 'nav-menu'}>\n                       \n                        {userData.user && (\n                       <li className='nav-item'>\n                           <Link to={`/chat?name=${userData.user.name}&room=chatroom`} className='nav-links' onClick={closeMobileMenu}>\n                               Chat\n                           </Link>\n                       </li>\n                       )}\n                       {userData.user && (\n                       <li className='nav-item'>\n                           <Link to=\"/leaderboard\" className='nav-links' onClick={closeMobileMenu}>\n                               Leaderboards\n                           </Link>\n                       </li>\n                       )}\n                       {userData.user && (\n                       <li className='nav-item'>\n                           <a href='#Popular' className='nav-links' onClick={closeMobileMenu}>\n                               Games\n                           </a>\n                       </li>\n                       )}\n                       <AuthOptions />\n                   </ul>\n               </div>\n           </nav>\n        </>\n    )\n}\n\nexport default NavBar;",["92","93"],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/components/Welcome/Welcome.js",[],["94","95"],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/components/GameCards/GameItem.js",[],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/App.js",["96"],"import './App.css';\nimport React, { useState, useEffect } from 'react';\nimport Axios from 'axios';\nimport {BrowserRouter as Router, Switch, Route} from 'react-router-dom';\nimport UserContext from './context/UserContext'\nimport Login from './components/Auth/Login';\nimport Register from \"./components/Auth/Register\";\nimport Navbar from './components/NavBar/NavBar';\nimport Home from './components/pages/Home';\nimport Chat from './components/Chat/Chat/Chat';\nimport Leaderboard from './components/pages/Leaderboard';\n\nfunction App() {\n  const [userData, setUserData] = useState({\n    token: undefined,\n    user: undefined\n  });\n\n  useEffect(() => {\n    const checkLoggedIn = async () => {\n      let token = localStorage.getItem(\"auth-token\");\n      if(token === null) { // If the user is logging in for the first time, there wont be a token present in the local storage yet\n        localStorage.setItem(\"auth-token\", \"\");\n        token = \"\";\n      }\n      const tokenRes = await Axios.post(\"http://localhost:5000/users/tokenIsValid\", null, \n                                        {headers : {\"x-auth-token\" : token}});\n      if(tokenRes.data){\n        const userRes = await Axios.get(\"http://localhost:5000/users/\", {headers : {\"x-auth-token\" : token}});\n        setUserData({token, user : userRes.data});\n      }\n    }\n  }, []);\n\n  return (\n    <>\n    <Router>\n      <UserContext.Provider value={{userData, setUserData}}>\n        <Navbar />\n        <Switch>\n          <Route path='/' exact component={Home} />\n          <Route path='/register' component={Register} />\n          <Route path='/login' component={Login} />\n          <Route path='/chat' component={Chat} />\n          <Route path='/leaderboard' component={Leaderboard} />\n        </Switch>\n      </UserContext.Provider>  \n    </Router>\n    </>\n  );\n}\n\nexport default App;\n",["97","98"],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/components/pages/Home.js",["99"],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/components/Buttons/Buttons.js",[],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/index.js",[],["100","101"],"/home/dhruval/Desktop/Projects/Sem-3/WebTech/Main-Branch/GamerParadise/React-Client/src/components/Foote